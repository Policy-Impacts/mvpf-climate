.             qui ds draw_number, not 
.             global estimates_${name} = r(varlist)
.             
.             mkmat ${estimates_${name}}, matrix(draws_${name}) rowna
> mes(draw_number)
.         restore
. *   }
.     local ests ${estimates_${name}}
.     foreach var in `ests' {
  2.         matrix temp = draws_${name}["${draw_number}", "`var'"]
  3.         local `var' = temp[1,1]
  4.     }
. }

. if "`bootstrap'" == "no" {
